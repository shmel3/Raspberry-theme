{
	"name": "Raspberry Cold",
	"type": "dark",
	"colors": {
		"editor.background": "#222222",
		"editor.foreground": "#bdb0b0",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	
	"tokenColors": [
		{
			"settings": {
				"foreground": "#c3d1d1"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7c7c7c"
			}
		},

		{
			"name": "Numeric, character constants",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.language"
			],
			"settings": {
				"foreground": "#af6ac0"
			}
		},

		{
			"name": "Strings",
			"scope": [
				"string",
				"string.unquoted.label.js"

			],
			"settings": {
				"foreground": "#77a163"
			}

		},

		{
			"name": "Regexp, escape character",
			"scope": [
				"constant.regexp",
				"constant.other",
				"constant.character.escape",
				"string.regexp"
			],
			"settings": {
				"foreground": "#57a5d3"
			}
		},

		{
			"name": "Variables, objects",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#a7d6d8"
			}
		},

		{
			"name": "Functions, methods",
			"scope": [
				"entity.name.function",
				"entity.name.method",
				"meta.function-call",
				"variable.function",
				"support.function"
			],
			"settings": {
				"foreground": "#bb4872"
			}
		}
	]
}