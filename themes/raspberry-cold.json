{
	"$schema": "vscode://schemas/color-theme",
	"name": "Raspberry Cold",
	"type": "dark",
	"colors": {
		"editor.foreground": "#bdb0b0",
		"activityBarBadge.background": "#9e2b61",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	
	"tokenColors": [
		{
			"settings": {
				"foreground": "#c3d1d1"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7c7c7c"
			}
		},

		{
			"name": "Numeric, character constants",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.language"
			],
			"settings": {
				"foreground": "#af6ac0"
			}
		},

		{
			"name": "Strings",
			"scope": [
				"string",
				"string.unquoted.label.js"

			],
			"settings": {
				"foreground": "#77a163"
			}

		},

		{
			"name": "Regexp, escape character",
			"scope": [
				"constant.regexp",
				"constant.other",
				"constant.character.escape",
				"string.regexp",
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp"
			],
			"settings": {
				"foreground": "#57a5d3"
			}
		},

		{
			"name": "Variables, objects, object keys",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"entity.name.variable",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#cecece"//"#4c7458"
			}
		},

		{
			"name": "Functions, methods",
			"scope": [
				"entity.name.method",
				"meta.function-call",
				"variable.function",
				"support.function",
				"entity.name.function",
                "meta.method-call.java meta.method",
                "meta.method.groovy",
                "support.function.any-method.lua",
                "keyword.operator.function.infix"
			],
			"settings": {
				"foreground": "#377e43" //"#8d682c"
			}
		},

		{
			"name": "Keywords, built-in types",
			"scope": [
				"keyword.control",
				"keyword.operator",
				"keyword.other",
				"storage.type.function",
				"storage.type.js",
				"keyword.primitive-datatypes.swift",
				"keyword.type.cs",
			],
			"settings": {
				"foreground": "#bb4872",
			}
		},

		{
			"name": "Modifiers",
			"scope": [
				"storage.modifier",
				"storage.modifier.js",
			],
			"settings": {
				"foreground": "#bb4872",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Support type",
			"scope": "support.type",
			"settings": {
				"foreground": "#bb4872"
			}
		},

		{
			"name": "Class types",
			"scope": [
				"support.class",
				"entity.name.type",
				"entity.name.class",
				"entity.name.class.forward-decl",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy",
                "meta.protocol-list.objc",
                "meta.return-type.objc",
                "source.go storage.type",
                "source.groovy storage.type",
                "source.java storage.type",
                "source.powershell entity.other.attribute-name",
                "storage.class.std.rust",
                "storage.type.attribute.swift",
                "storage.type.c",
                "storage.type.core.rust",
                "storage.type.objc",
                "storage.type.php",
                "storage.type.haskell",
                "storage.type.ocaml"
			],
			"settings": {
				"foreground": "#72a7da",
			}
		},

		{
			"name": "HTML tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#bb4872"
			}
		},

		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#72a7da"
			}
		},

		{
            "name": "Brackets, braces, parens, etc.",
            "scope": [
                "keyword.operator.other.powershell",
                "keyword.other.statement-separator.powershell",
                "meta.brace.round",
                "meta.function-call punctuation",
                "punctuation.definition.arguments.begin",
                "punctuation.definition.arguments.end",
                "punctuation.definition.entity.begin",
                "punctuation.definition.entity.end",
                "punctuation.definition.tag.cs",
                "punctuation.definition.type.begin",
                "punctuation.definition.type.end",
                "punctuation.section.scope.begin",
                "punctuation.section.scope.end",
                "storage.type.generic.java",
                "string.template meta.brace",
				"string.template punctuation.accessor"
            ],
            "settings": {
                "foreground": "#bbb8b8"
            }
        },
	]
}